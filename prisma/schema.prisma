// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("GCP_MYSQL_URL")
}

model Session {
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?
}

model ShopVisionatiApiKeys {
  shop_id             String    @id
  visionati_api_key   String
}

model ShopAutoImageDescriptions {
  shop_id String    @id
}
